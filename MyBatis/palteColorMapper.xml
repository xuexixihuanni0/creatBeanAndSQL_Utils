<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- PALTE_COLOR -->
<mapper namespace="ABCDEFG.palteColor">
    <!-- 结果映射 -->
    <resultMap id="palteColorMap" type="PalteColorEntity"></resultMap>
    
    <!-- 表所有字段 -->
    <sql id="allColumns">
        ID, COLOR
    </sql>
    
    <!-- 批量插入数据 -->
    <insert id="insertPalteColors" parameterType="list">
        INSERT INTO PALTE_COLOR (<include refid="allColumns" />)
        VALUES (
        <foreach collection="list" index="index" item="item" separator=",">
            (
                #{item.id},
                #{item.color}
            )
        </foreach>
    </insert>
    
</mapper>