<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- CONGESTION_CHANNELINFO -->
<mapper namespace="ABCDEFG.congestionChannelinfo">
    <!-- 结果映射 -->
    <resultMap id="congestionChannelinfoMap" type="CongestionChannelinfoEntity"></resultMap>
    
    <!-- 表所有字段 -->
    <sql id="allColumns">
        ID, CHANNELEVT_INFOID, CHANNEL_ID, EVT_ID, TIME
    </sql>
    
    <!-- 批量插入数据 -->
    <insert id="insertCongestionChannelinfos" parameterType="list">
        INSERT INTO CONGESTION_CHANNELINFO (<include refid="allColumns" />)
        VALUES (
        <foreach collection="list" index="index" item="item" separator=",">
            (
                #{item.id},
                #{item.channelevtInfoid},
                #{item.channelId},
                #{item.evtId},
                #{item.time}
            )
        </foreach>
    </insert>
    
</mapper>