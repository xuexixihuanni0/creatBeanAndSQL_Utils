<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- CONGESTION_EVT -->
<mapper namespace="ABCDEFG.congestionEvt">
    <!-- 结果映射 -->
    <resultMap id="congestionEvtMap" type="CongestionEvtEntity"></resultMap>
    
    <!-- 表所有字段 -->
    <sql id="allColumns">
        ID, EVT_ID, eventId, evtBeginEnd, evtType, POSX, POSY, gpsPosLat, 
        gpsPosLong, queueLength, queueTime, eventImageSize, eventImage, TIME
    </sql>
    
    <!-- 批量插入数据 -->
    <insert id="insertCongestionEvts" parameterType="list">
        INSERT INTO CONGESTION_EVT (<include refid="allColumns" />)
        VALUES (
        <foreach collection="list" index="index" item="item" separator=",">
            (
                #{item.id},
                #{item.evtId},
                #{item.eventId},
                #{item.evtBeginEnd},
                #{item.evtType},
                #{item.posx},
                #{item.posy},
                #{item.gpsPosLat},
                #{item.gpsPosLong},
                #{item.queueLength},
                #{item.queueTime},
                #{item.eventImageSize},
                #{item.eventImage},
                #{item.time}
            )
        </foreach>
    </insert>
    
</mapper>