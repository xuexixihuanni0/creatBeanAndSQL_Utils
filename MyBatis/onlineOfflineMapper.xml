<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- ONLINE_OFFLINE -->
<mapper namespace="ABCDEFG.onlineOffline">
    <!-- 结果映射 -->
    <resultMap id="onlineOfflineMap" type="OnlineOfflineEntity"></resultMap>
    
    <!-- 表所有字段 -->
    <sql id="allColumns">
        ID, TYPE, GATEWAY_MANUFACTURERS, GATEWAY_INSTALLATIONAREA, GATEWAY_INSTALLPOINT, GATEWAY_VERSIONNUMBER, GATEWAY_DEVICE, GATEWAY_INSTALLSERIAL, 
        THINGS_ID, SEQUENCE, TIME
    </sql>
    
    <!-- 批量插入数据 -->
    <insert id="insertOnlineOfflines" parameterType="list">
        INSERT INTO ONLINE_OFFLINE (<include refid="allColumns" />)
        VALUES (
        <foreach collection="list" index="index" item="item" separator=",">
            (
                #{item.id},
                #{item.type},
                #{item.gatewayManufacturers},
                #{item.gatewayInstallationarea},
                #{item.gatewayInstallpoint},
                #{item.gatewayVersionnumber},
                #{item.gatewayDevice},
                #{item.gatewayInstallserial},
                #{item.thingsId},
                #{item.sequence},
                #{item.time}
            )
        </foreach>
    </insert>
    
</mapper>