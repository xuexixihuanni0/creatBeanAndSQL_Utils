<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- DEVICE_STATUS -->
<mapper namespace="ABCDEFG.deviceStatus">
    <!-- 结果映射 -->
    <resultMap id="deviceStatusMap" type="DeviceStatusEntity"></resultMap>
    
    <!-- 表所有字段 -->
    <sql id="allColumns">
        ID, DEVICE_ID, TYPE, SEQUENCE, TIME
    </sql>
    
    <!-- 批量插入数据 -->
    <insert id="insertDeviceStatuss" parameterType="list">
        INSERT INTO DEVICE_STATUS (<include refid="allColumns" />)
        VALUES (
        <foreach collection="list" index="index" item="item" separator=",">
            (
                #{item.id},
                #{item.deviceId},
                #{item.type},
                #{item.sequence},
                #{item.time}
            )
        </foreach>
    </insert>
    
</mapper>