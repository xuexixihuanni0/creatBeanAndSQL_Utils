<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- ONLINEOFFLINE_THINGS -->
<mapper namespace="ABCDEFG.onlineofflineThings">
    <!-- 结果映射 -->
    <resultMap id="onlineofflineThingsMap" type="OnlineofflineThingsEntity"></resultMap>
    
    <!-- 表所有字段 -->
    <sql id="allColumns">
        ID, THINGS_ID, TYPE, MANUFACTURERS, installationArea, installPoint, versionNumber, DEVICE, 
        installSerial, TIME
    </sql>
    
    <!-- 批量插入数据 -->
    <insert id="insertOnlineofflineThingss" parameterType="list">
        INSERT INTO ONLINEOFFLINE_THINGS (<include refid="allColumns" />)
        VALUES (
        <foreach collection="list" index="index" item="item" separator=",">
            (
                #{item.id},
                #{item.thingsId},
                #{item.type},
                #{item.manufacturers},
                #{item.installationArea},
                #{item.installPoint},
                #{item.versionNumber},
                #{item.device},
                #{item.installSerial},
                #{item.time}
            )
        </foreach>
    </insert>
    
</mapper>