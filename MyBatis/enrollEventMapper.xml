<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- ENROLL_EVENT -->
<mapper namespace="ABCDEFG.enrollEvent">
    <!-- 结果映射 -->
    <resultMap id="enrollEventMap" type="EnrollEventEntity"></resultMap>
    
    <!-- 表所有字段 -->
    <sql id="allColumns">
        ID, GATEWAY_MANUFACTURERS, GATEWAY_INSTALLATIONAREA, GATEWAY_INSTALLPOINT, GATEWAY_VERSIONNUMBER, GATEWAY_DEVICE, GATEWAY_INSTALLSERIAL, ROLE, 
        SOFTWARE, HARDWARE, IPV4, POSITION, METHODS, EVENTS, SEQUENCE, TIME
    </sql>
    
    <!-- 批量插入数据 -->
    <insert id="insertEnrollEvents" parameterType="list">
        INSERT INTO ENROLL_EVENT (<include refid="allColumns" />)
        VALUES (
        <foreach collection="list" index="index" item="item" separator=",">
            (
                #{item.id},
                #{item.gatewayManufacturers},
                #{item.gatewayInstallationarea},
                #{item.gatewayInstallpoint},
                #{item.gatewayVersionnumber},
                #{item.gatewayDevice},
                #{item.gatewayInstallserial},
                #{item.role},
                #{item.software},
                #{item.hardware},
                #{item.ipv4},
                #{item.position},
                #{item.methods},
                #{item.events},
                #{item.sequence},
                #{item.time}
            )
        </foreach>
    </insert>
    
</mapper>